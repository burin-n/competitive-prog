#include <iostream>
#include <string>
using namespace std;

string s1,s2;

bool compute(){
	int i=0,j,p=0;
	while(p<s1.length() && i<s2.length()){
		j=i;
		while(j+1<s2.length() && s2[j+1] == s2[i]) j++;
		cout << i << '|' << j <<'\\';
		cout << p <<endl;
		if(s2[i] == s1[p]){ // equal
			if(j>i){ // more than one 
				if(p+1<s1.length()){
					if(!(s1[p+1] == '*' || s1[p+1] == '+'))
						return false;	
					p = p+2;
					i=j+1;
				}
				else return false;  
			}
			else{
				if(p+1 < s1.length() && (s1[p+1] == '*' || s1[p+1] == '+' ))
					p = p+2,i=j+1;
				else p++,i=j+1;
			}
		}
		else{
			if(p+1>=s1.length()) return false;	
			else if(s1[p+1] == '*') p+=2;	
			else return false;
		}
	}
	if(p>=s1.length() && i>=s2.length()) return true;
	else if(p<s1.length() && i>=s2.length()){
		while(p+2 < s1.length()){
			if(s1[p+2]=='*') p+=2;
			else return false;
		}
	return true;
	}	
	else return false;
}

int main(){
	
	int T;
	cin >> T;	
	while(T--){
		cin >> s1 >> s2;
		if(compute()) cout<<"YES"<<endl;			
		else cout<<"NO"<<endl;

	}
	return 0;
}

